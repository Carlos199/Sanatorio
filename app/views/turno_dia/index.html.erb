<p id="notice"><%= notice %></p>

<h1>Turno Dia</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @turno_dia.each do |turno_dias| %>
      <tr>
        <td><%= turno_dias.nombre %></td>
        <td><%= link_to 'Show', turno_dias %></td>
        <td><%= link_to 'Edit', edit_turno_dias_path(turno_dias) %></td>
        <td><%= link_to 'Destroy', turno_dias, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Turno Dias', new_turno_dias_path %>
