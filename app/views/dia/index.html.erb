<p id="notice"><%= notice %></p>

<h1>Dia</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dia.each do |dias| %>
      <tr>
        <td><%= dias.codigo %></td>
        <td><%= dias.nombre %></td>
        <td><%= link_to 'Show', dias %></td>
        <td><%= link_to 'Edit', edit_dias_path(dias) %></td>
        <td><%= link_to 'Destroy', dias, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dias', new_dias_path %>
