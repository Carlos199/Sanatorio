<p id="notice"><%= notice %></p>

<h1>Tipo Consults</h1>

<table>
  <thead>
    <tr>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_consults.each do |tipo_consult| %>
      <tr>
        <td><%= tipo_consult.descripcion %></td>
        <td><%= link_to 'Show', tipo_consult %></td>
        <td><%= link_to 'Edit', edit_tipo_consult_path(tipo_consult) %></td>
        <td><%= link_to 'Destroy', tipo_consult, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo Consult', new_tipo_consult_path %>
