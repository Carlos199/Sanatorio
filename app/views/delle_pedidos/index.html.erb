<h1>Delle Pedidos</h1>

<table class="table">
  <thead>
    <tr>
      <th>Pedido</th>
      <th>Insumo</th>
      <th>Cantidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @delle_pedidos.each do |delle_pedido| %>
      <tr>
        <td><%= delle_pedido.pedido %></td>
        <td><%= delle_pedido.insumo %></td>
        <td><%= delle_pedido.cantidad %></td>
        <td><%= link_to 'Show', delle_pedido %></td>
        <td><%= link_to 'Edit', edit_delle_pedido_path(delle_pedido) %></td>
        <td><%= link_to 'Destroy', delle_pedido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

