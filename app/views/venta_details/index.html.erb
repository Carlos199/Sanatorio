<p id="notice"><%= notice %></p>

<h1>Venta Details</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Qty</th>
      <th>Price</th>
      <th>Venta</th>
      <th>Insumo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venta_details.each do |venta_detail| %>
      <tr>
        <td><%= venta_detail.number %></td>
        <td><%= venta_detail.qty %></td>
        <td><%= venta_detail.price %></td>
        <td><%= venta_detail.venta_id %></td>
        <td><%= venta_detail.insumo_id %></td>
        <td><%= link_to 'Show', venta_detail %></td>
        <td><%= link_to 'Edit', edit_venta_detail_path(venta_detail) %></td>
        <td><%= link_to 'Destroy', venta_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venta Detail', new_venta_detail_path %>
